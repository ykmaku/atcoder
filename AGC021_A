#include <iostream>
#include <string>
#include <algorithm>
#include <cstdio>
#include <vector>
#include <queue>
#include <set>
#include <numeric>
#include <cmath>

using namespace std;

typedef long long int lld;

const lld mod = 1e9+7;
const lld INF = 1e9;
//const lld MAXN = 1e9;

int main()
{
	string n;
	cin >> n;
	int digit = n.size();
	// cout << n.size() << endl;
	// cout << n[0] << endl;
	for(int i = 0; i < n.size(); i++)
	{
		if (n[i] != '9')
		{
			break;
		}
		if (i == n.size()-1)
		{
			cout << 9 * n.size() << endl;
			return 0;
		}
	}

	for(int i = 1; i < n.size(); i++)
	{
		if (n[i] != '9')
		{
			break;
		}
		if (i == n.size()-1)
		{
			cout << 9 * (n.size()-1) + (int)(n[0]-'0') << endl;
			return 0;
		}
	}

	if (digit == 1)
	{
		cout << (int)(n[0]-'0') << endl;
		return 0;
	}
	cout << (digit-1) * 9 + (int)(n[0]-'0')-1 << endl;
	return 0;
}
