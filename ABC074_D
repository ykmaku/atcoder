#include <iostream>
#include <string>
#include <algorithm>
#include <cstdio>
#include <vector>
#include <queue>
#include <set>
#include <numeric>
#include <cmath>

using namespace std;

typedef long long int lld;

const lld mod = 1e9+7;
const lld INF = 1e9;
//const lld MAXN = 1e9;


int main()
{
	int n;
	cin >> n;
	int a[300][300] = {0};

	for(int i = 0; i < n; i++)
	{
		for(int j = 0; j < n; j++)
		{
			cin >> a[i][j];
		}
	}

	lld dist[300][300] = {0};

	for(int i = 0; i < n; i++)
	{
		for(int j = 0; j < n; j++)
		{
			dist[i][j] = a[i][j];
		}
	}

	for(int k = 0; k < n; k++)
	{
		for(int i = 0; i < n; i++)
		{
			for(int j = 0; j < n; j++)
			{
				if (dist[i][j] > dist[i][k] + dist[k][j])
				{
					dist[i][j] = dist[i][k] + dist[k][j];
				}
			}
		}
	}

	for(int i = 0; i < n; i++)
	{
		for(int j = 0; j < n; j++)
		{
			if (dist[i][j] != a[i][j])
			{
				cout << -1 << endl;
				return 0;
			}
		}
	}

	bool ch[300][300] = {false};
	for(int k = 0; k < n; k++)
	{
		for(int i = 0; i < n; i++)
		{
			for(int j = 0; j < n; j++)
			{
				if (dist[i][j] >=  dist[i][k] + dist[k][j] && i != j && i != k && j != k)
				{
					ch[i][j] = true;
				}
			}
		}
	}


	lld sum = 0;

	for(int i = 0; i < n; i++)
	{
		for(int j = 0; j < n; j++)
		{
			if (!ch[i][j])
			{
				sum += dist[i][j];
			}
		}
	}
	
	cout << sum / 2 << endl;

	return 0;
}
