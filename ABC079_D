#include <iostream>
#include <string>
#include <algorithm>
#include <cstdio>
#include <vector>
#include <queue>
#include <set>
#include <numeric>
#include <cmath>

using namespace std;

typedef long long int lld;

const lld mod = 1e9+7;
const lld INF = 1e9;
//const lld MAXN = 1e9;

int main()
{
    int h,w;
    int c[10][10] = {0};
    int a[200][200] = {0};

    cin >> h >> w;
    for(int i = 0; i < 10; i++)
    {
        for(int j = 0; j < 10; j++)
        {
            cin >> c[i][j];
        }
    }
    for(int i = 0; i < h; i++)
    {
        for(int j = 0; j < w; j++)
        {
            cin >> a[i][j];
        }
    }

    int dist[10][10] = {0};
    for(int i = 0; i < 10; i++)
    {
        for(int j = 0; j < 10; j++)
        {
            dist[i][j] = c[i][j];
        }
    }

    for(int k = 0; k < 10; k++)
    {
        for(int i = 0; i < 10; i++)
        {
            for(int j = 0; j < 10; j++)
            {
                if (dist[i][j] > dist[i][k] + dist[k][j])
                {
                    dist[i][j] = dist[i][k] + dist[k][j];
                }
            }
        }
    }

    int sol = 0;

    for(int i = 0; i < h; i++)
    {
        for(int j = 0; j < w; j++)
        {
            if (a[i][j] == 0 || (1 < a[i][j] && a[i][j] < 10))
            {
                sol += dist[a[i][j]][1];
            }
        }
    }

    cout << sol << endl;

    return 0;
}
